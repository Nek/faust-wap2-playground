{
  "name": "faust-wap2",
  "version": "0.1.0",
  "watch" : {
    "compile" : "dsp/melody.dsp"
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "vite --open",
    "build": "tsc && vite build",
