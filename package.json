    "process": false,
    "setTimeout": false,
    "util": false
  },
  "devDependencies": {
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.2"
  }
}
